.thumbs
  background-color:#ffffff
  font-family: 'Roboto', sans-serif
  margin-bottom: 3%
  color: #4a4a49
  h2
    text-align: center
    font-size: 2rem
    text-transform: uppercase
    font-weight: 400
  p
    margin-bottom: 6%
    line-height: 1.5
    font-weight: 400
  .cont-boton-azul
    text-align: center
  .que-son
  .como-voto
  .donde-voto
    width: 200px
    height: 200px
    background-repeat: no-repeat
    display: auto
    position: relative
    background-size: contain
    margin: 0 auto
  .que-son-cont
    margin-left: 3.62%
  .donde-voto-cont
    margin-right: 3.62%
  .que-son
    background-image: unquote("url('/ext/lib/site/thumbs-voto/foros-vecinales-icono.svg')")
    background-position: center
  .como-voto
    background-image: unquote("url('/ext/lib/site/thumbs-voto/voto6.svg')")
    background-position: center
  .donde-voto
    background-image: unquote("url('/ext/lib/site/thumbs-voto/location3.svg')")
    background-position: center
