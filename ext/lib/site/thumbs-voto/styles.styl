.thumbs
  background-color:#ffffff
  font-family: 'Roboto', sans-serif
  margin-bottom: 3%
  h2
    text-align: center
  .que-son
  .como-voto
  .donde-voto
    width: 150px
    height: 150px
    background-repeat: no-repeat
    display: auto
    position: relative
    margin: 0 auto
  .que-son-cont
    margin-left: 3.62%
  .donde-voto-cont
    margin-right: 3.62%
  .que-son
    background-image: unquote("url('/ext/lib/site/thumbs-voto/QueSonFVIcono.svg')")
  .como-voto
    background-image: unquote("url('/ext/lib/site/thumbs-voto/VotoIcono.svg')")
  .donde-voto
    background-image: unquote("url('/ext/lib/site/thumbs-voto/LocationIcono.svg')")
  .boton-azul
    background-color: #1791be
    height: 77px
    width: 200px
    color: white
    box-shadow: 0 6px 4px -2px unquote("rgba(0, 0, 0, 0.24)"), 0 0 2px 0 unquote("rgba(0, 0, 0, 0.12)")
    color white
    margin-top: 10%
    align-items: center
    justify-content: center
    font-weight: 500
    font-size: 1.88rem
    a
    p
      margin: 0
