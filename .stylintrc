{
    "blocks": "never",
    "brackets": "never",
    "colons": "never",
    "colors": false,
    "commaSpace": "always",
    "commentSpace": "always",
    "cssLiteral": false,
    "depthLimit": 3,
    "duplicates": true,
    "efficient": "always",
    "extendPref": "@extend",
    "globalDupe": true,
    "indentPref": 2,
    "leadingZero": "never",
    "maxErrors": 20,
    "maxWarnings": 20,
    "namingConvention": "lowercase-dash",
    "namingConventionStrict": true,
    "none": "never",
    "parenSpace": "never",
    "placeholder": false,
    "prefixVarsWithDollar": "never",
    "quotePref": "single",
    "stackedProperties": "never",
    "semicolons": "never",
    "sortOrder": "grouped",
    "universal": "never",
    "valid": true,
    "whitespace": true,
    "zeroUnits": "never",
    "zIndexNormalize": 5
}